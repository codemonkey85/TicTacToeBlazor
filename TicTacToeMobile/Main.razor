<Router AppAssembly="@GetType().Assembly"
        AdditionalAssemblies="new [] { typeof(TicTacToeBlazor.App).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(TicTacToeBlazor.Shared.MainLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(TicTacToeBlazor.Shared.MainLayout)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
